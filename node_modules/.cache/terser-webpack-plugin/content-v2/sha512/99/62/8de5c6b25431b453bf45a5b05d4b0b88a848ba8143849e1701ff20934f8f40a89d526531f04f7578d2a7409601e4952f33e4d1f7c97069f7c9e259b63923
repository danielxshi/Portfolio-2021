ent({src}){\n  let imgStyles={\n    width:100+\"%\",\n    height: \"auto\"\n  }\n  return <img src={src} style={imgStyles} alt=\"slide-img\"></img>;\n}\n\nexport default SliderContent;","import React, {useState} from \"react\";  \nimport '../../style/modules/_slider.scss'\nimport SliderContent from \"../../components/ProjectDetails/SliderContent\"\n\n\nfunction Slider(props){\n  let sliderArr = [<SliderContent src={props.img1}></SliderContent>, <SliderContent src={props.img2}></SliderContent>];\n  const [state, setState] = useState({\n    activeIndex: 0\n  })\n\n  const { activeIndex } = state\n\n  const [x, setX] = useState(0)\n\n\n  // Next slide\n  const nextSlide = () => {\n\n    // Translate\n    x === -100 *(sliderArr.length-1) ? setX(0):setX(x - 100);\n\n    // Return state\n    if(activeIndex === sliderArr.length-1){\n\n      return setState({\n        activeIndex: 0\n      })\n    }\n\n    setState({\n      activeIndex: activeIndex + 1\n    })\n  };\n\n  // Previous slide\n  const prevSlide = () => {\n\n    // Translate\n    x === 0 ? setX(-100 * (sliderArr.length - 1)) : setX(x + 100);\n    \n    // Return state\n    if(activeIndex === 0){\n\n      x === 0 ? setX(-100 * (sliderArr.length - 1)) : setX(x - 100);\n      return setState({\n  \n        activeIndex: (sliderArr.length)-1\n        \n        // activeIndex: 0\n      })\n    }\n    setState({\n      activeIndex: activeIndex - 1\n    })\n  };\n\n\n  // arraySize\n  var totalItems = sliderArr.length;\n\n  // console.log(sliderArr.length);\n\n  \n  return(\n    <div className=\"slide-container\">\n      <div className=\"slider\">\n        {sliderArr.map((item, index)=> {\n          return(\n            <div key={index} className=\"slide\" style={{transform: `translateX(${x}%)`}}>\n              {item}\n            </div>\n          );\n        })}\n          <button id=\"prevSlide\" onClick={prevSlide}>\n          {/* left */}\n          </button>\n          <button id=\"nextSlide\" onClick={nextSlide}>\n          {/* right */}\n          </button>\n      </div>\n      <p className=\"slide-pg-count\">\n        0{activeIndex + 1} /\n        0{totalItems}\n      \n      </p> \n\n\n    </div>\n  );\n\n}\n\nexport default Slider;","export default __webpack_public_path__ + \"static/media/persona-isometric.e2a701b9.png\";","export default __webpack_public_path__ + \"static/media/sketch-build-guide.77df5955.webp\";","export default __webpack_public_path__ + \"static/media/sketch-build-guide-2.5258b2ce.webp\";","export default __webpack_public_path__ + \"static/media/sketch-prod-detail.9d8e7d7b.webp\";","export default __webpack_public_path__ + \"static/media/sketch-prod-list.7d37ac98.webp\";","export default __webpack_public_path__ + \"static/media/wireframe-overview.e03c5c24.webp\";","export default __webpack_public_path__ + \"static/media/pattern-lib.7f777a19.png\";","export default __webpack_public_path__ + \"static/media/primary-btn-neu-guide.6b9c31c3.png\";","export default __webpack_public_path__ + \"static/media/secondary-btn-neu-guide.22f82eba.png\";","export default __webpack_public_path__ + \"static/media/typography.11f93839.webp\";","export default __webpack_public_path__ + \"static/media/kerning.d2f8b4ba.webp\";","export default __webpack_public_path__ + \"static/media/sitemap-lite.54c66ccf.png\";","export default \"data:image/png;base64,\"","export default __webpack_public_path__ + \"static/media/cliq-logo.9eb77e20.webp\";","export default __webpack_public_path__ + \"static/media/product-detail-r2.3ba448cb.png\";","export default __webpack_public_path__ + \"static/media/cmatrix-blk-bg.0ca2f075.gif\";","export default __webpack_public_path__ + \"static/media/cliq-white-text-anim.6c3dd7d8.gif\";","import React from 'react';\n\n\n// Stylesheets\nimport './style/pages/_projects.scss';\n\n// Import components\nimport SectionHelperText from \"./components/ProjectDetails/SectionHelperText\";\nimport SectionHelperGraphic from \"./components/ProjectDetails/SectionHe