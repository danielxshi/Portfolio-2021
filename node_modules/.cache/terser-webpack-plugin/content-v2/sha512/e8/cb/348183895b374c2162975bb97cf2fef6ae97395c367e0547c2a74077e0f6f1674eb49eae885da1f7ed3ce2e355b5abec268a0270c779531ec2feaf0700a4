n        activeIndex: 0\n      })\n    }\n\n    setState({\n      activeIndex: activeIndex + 1\n    })\n  };\n\n  // Previous slide\n  const prevSlide = () => {\n\n    // Translate\n    x === 0 ? setX(-100 * (sliderArr.length - 1)) : setX(x + 100);\n    \n    // Return state\n    if(activeIndex === 0){\n\n      x === 0 ? setX(-100 * (sliderArr.length - 1)) : setX(x - 100);\n      return setState({\n  \n        activeIndex: (sliderArr.length)-1\n        \n      })\n    }\n    setState({\n      activeIndex: activeIndex - 1\n    })\n  };\n\n\n  // arraySize\n  var totalItems = sliderArr.length;\n  return(\n    <div className=\"slide-container\">\n      <div className=\"slider\">\n        {sliderArr.map((item, index)=> {\n          return(\n            <div key={index} className=\"slide\" style={{transform: `translateX(${x}%)`}}>\n              {item}\n            </div>\n          );\n        })}\n          <button id=\"prevSlide\" onClick={prevSlide}>\n          {/* left */}\n          </button>\n          <button id=\"nextSlide\" onClick={nextSlide}>\n          {/* right */}\n          </button>\n      </div>\n      <p className=\"slide-pg-count\">\n        0{activeIndex + 1} /\n        0{totalItems}\n      </p> \n    </div>\n  );\n\n}\n\nexport default Slider;","import React from 'react';\n\nfunction ModuleFiftyGRight(props) {\n    return (\n        <section className=\"mod-50 sect-1 sect-last\">\n            <div className=\"grid-row-space grid-container\">\n                <div className=\"proj-col-contain\">\n                    <div className=\"sub-container grid-col-space\">\n                        <div className=\"span-7\">\n                            <h3 className=\"rel-margin-bot\">{props.title}</h3>\n                            <p className=\"rel-margin-bot\">{props.p1}</p>\n                            <p>{props.p2}</p>\n                        </div>\n                        <div className=\"img-fit gcs-9-5\">\n                            <img loading=\"lazy\" src={props.img} alt={props.alt}/>\n                            <p className=\"citation\">{props.citation}</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </section>\n    );\n}\n\nexport default ModuleFiftyGRight;","export default __webpack_public_path__ + \"static/media/sketch-minigame-m-lrg.add4d3b8.jpg\";","export default __webpack_public_path__ + \"static/media/sketch-overview-2500x.323a6f70.png\";","export default __webpack_public_path__ + \"static/media/i1-friend-list.3485b8db.webp\";","export default __webpack_public_path__ + \"static/media/i1-mg1.19843705.webp\";","export default __webpack_public_path__ + \"static/media/i1-mg2.d14c59c1.webp\";","export default __webpack_public_path__ + \"static/media/wireframe-overview.66f7cbf3.webp\";","export default __webpack_public_path__ + \"static/media/persona-bstep.a439a004.webp\";","export default \"data:image/png;base64,\"","export default __webpack_public_path__ + \"static/media/blob-neu.1e34bb5b.png\";","export default __webpack_public_path__ + \"static/media/persona-graphic.45e94d65.png\";","export default __webpack_public_path__ + \"static/media/bstep-mg1-b.de682618.jpg\";","export default __webpack_public_path__ + \"static/media/bstep-mg1-a.9f1bb9cb.jpg\";","export default __webpack_public_path__ + \"static/media/bstep-mg2-b.671bf57f.jpg\";","export default __webpack_public_path__ + \"static/media/bstep-mg2-a.3f1bc9bb.jpg\";","export default __webpack_public_path__ + \"static/media/bstep-h-b.86bd5424.jpg\";","export default __webpack_public_path__ + \"static/media/bstep-h-a.6f960cfc.jpg\";","export default __webpack_public_path__ + \"static/media/field-test-mcu.e8ee1016.jpg\";","import React from 'react';\n\n// Stylesheets\nimport './style/pages/_projects.scss';\n\n// Import placeholders\nimport img34 from './images/placeholder/placeholder-3x4.png';\nimport img43 from './images/placeholder/placeholder-4x3.png';\nimport img2x1 from './im