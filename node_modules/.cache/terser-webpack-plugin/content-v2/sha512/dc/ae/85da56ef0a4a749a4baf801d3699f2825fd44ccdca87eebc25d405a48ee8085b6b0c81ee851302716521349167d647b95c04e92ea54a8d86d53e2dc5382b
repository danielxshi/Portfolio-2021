 arraySize\n  var totalItems = sliderArr.length;\n  return(\n    <div className=\"slide-container\">\n      <div className=\"slider\">\n        {sliderArr.map((item, index)=> {\n          return(\n            <div key={index} className=\"slide\" style={{transform: `translateX(${x}%)`}}>\n              {item}\n            </div>\n          );\n        })}\n          <button id=\"prevSlide\" onClick={prevSlide}>\n          {/* left */}\n          </button>\n          <button id=\"nextSlide\" onClick={nextSlide}>\n          {/* right */}\n          </button>\n      </div>\n      <p className=\"slide-pg-count\">\n        0{activeIndex + 1} /\n        0{totalItems}\n      </p> \n    </div>\n  );\n\n}\n\nexport default Slider;","import React from 'react';\n\nfunction ModuleFiftyGRight(props) {\n    return (\n        <section className=\"mod-50 sect-1 sect-last\">\n            <div className=\"grid-row-space grid-container\">\n                <div className=\"proj-col-contain\">\n                    <div className=\"sub-container grid-col-space\">\n                        <div className=\"span-7\">\n                            <h3 className=\"rel-margin-bot\">{props.title}</h3>\n                            <p className=\"rel-margin-bot\">{props.p1}</p>\n                            <p>{props.p2}</p>\n                        </div>\n                        <div className=\"img-fit gcs-9-5\">\n                            <img loading=\"lazy\" src={props.img} alt={props.alt}/>\n                            <p className=\"citation\">{props.citation}</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </section>\n    );\n}\n\nexport default ModuleFiftyGRight;","export default __webpack_public_path__ + \"static/media/sketch-minigame-m-lrg.add4d3b8.jpg\";","export default __webpack_public_path__ + \"static/media/sketch-overview-2500x.323a6f70.png\";","export default __webpack_public_path__ + \"static/media/i1-friend-l